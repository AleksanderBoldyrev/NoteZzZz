            <TextField fx:id="serverName" layoutX="53.0" layoutY="31.0" />
            <TextField fx:id="userName" layoutX="53.0" layoutY="85.0" />
            <PasswordField fx:id="password" layoutX="53.0" layoutY="141.0" />
            <Label layoutX="88.0" layoutY="14.0" text="ServerId (opt.))" />
            <Label layoutX="109.0" layoutY="62.0" text="Login" />
            <Label layoutX="99.0" layoutY="117.0" text="Password" />
            <Label fx:id="infoLabel" layoutX="22.0" layoutY="280.0" prefHeight="61.0" prefWidth="206.0" text="" />
